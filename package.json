{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-form": "^1.55.1",
    "@ant-design/pro-layout": "^6.5.0",
    "@antv/g6": "^4.6.4",
    "@antv/graphin": "^2.5.0",
    "@antv/graphin-components": "^2.4.0",
    "@umijs/preset-react": "1.x",
    "antd": "^4.18.9",
    "gg-editor": "^2.0.2",
    "mockjs": "^1.1.0",
    "umi": "^3.5.21"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.3.10",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
